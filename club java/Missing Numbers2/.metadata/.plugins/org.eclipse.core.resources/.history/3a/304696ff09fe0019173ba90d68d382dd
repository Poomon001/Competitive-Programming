package test;

public class test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 double A [][] = {
		            {1, 0.5, 0.33, 0.25},
		            {0.5, 0.33, 0.25, 0.2},
		            {0.33, 0.25, 0.2, 0.16},
		            {0.25, 0.2, 0.16, 0.14},
		    };

		    double B[][] = {
		            {1, 1, 1},
		            {1, 2, 2},
		            {1, 2, 3},
		            {1, 2, 3},
		    };
		    
		    double C[][] = new double [5][5];
		    

		            for (int i = 0; i <= 5 - 1; i++) {
		                for (int j = 0; j <= 5 - 1; j++) {
		                    C[i][j] = 0;
		                    for (int k = 0; k <= 5 - 1; k++) {
		                        C[i][j] = C[i][j] + (A[i][k] * B[k][j]);
		                    }//for

		                }//for
		            }//for
	

}
}
